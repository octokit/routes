  },
  "x-code-samples": [
    {
      "lang": "Shell",
      "source": "curl \\\n  -H\"Accept: application/vnd.github.v3+json\" \\\n  https://api.github.com/repos/octocat/hello-world/traffic/popular/paths"
    },
    {
      "lang": "JS",
      "source": "octokit.repos.getTopPaths({\n  owner: 'octocat',\n  repo: 'hello-world'\n})"
