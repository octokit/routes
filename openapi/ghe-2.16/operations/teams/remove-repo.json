      "description": "Setting to `application/vnd.github.v3+json` is recommended",
      "in": "header",
      "schema": {
        "type": "string",
        "default": "application/vnd.github.v3+json"
      }
    },
    {
      "name": "team_id",
      "description": "team_id parameter",
      "in": "path",
      "required": true,
      "schema": {
        "type": "integer"
      }
    },
    {
      "name": "owner",
      "description": "owner parameter",
      "in": "path",
      "required": true,
      "schema": {
        "type": "string"
      }
    },
    {
      "name": "repo",
      "description": "repo parameter",
