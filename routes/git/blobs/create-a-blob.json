{
  "name": "Create a blob",
  "enabledForApps": true,
  "method": "POST",
  "path": "/repos/:owner/:repo/git/blobs",
  "params": [
    {
      "name": "owner",
      "type": "string",
      "required": true,
      "description": "",
      "location": "url"
    },
    {
      "name": "repo",
      "type": "string",
      "required": true,
      "description": "",
      "location": "url"
    },
    {
      "name": "content",
      "type": "string",
      "description": "The new blob's content.",
      "required": true,
      "location": "body"
    },
    {
      "name": "encoding",
      "type": "string",
      "description": "The encoding used for `content`. Currently, `\"utf-8\"` and `\"base64\"` are supported.",
      "default": "\"utf-8\"",
      "required": false,
      "location": "body"
    }
  ],
  "description": "",
  "documentationUrl": "https://developer.github.com/v3/git/blobs/#create-a-blob"
}
